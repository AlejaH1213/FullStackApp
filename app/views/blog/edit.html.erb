<h2> Edit a robot warning sign </h2>
<%= form_with model: @blog, method: :patch do |form| %>

    <%= form.label :title %>
    <%= form.text_field :title %>

    <br>
    <%= form.label :content %>
    <%= form.text_field :content %>

    <br>
    <%= form.submit 'edit Your Robot Warning Sign' %>
  <% end %>